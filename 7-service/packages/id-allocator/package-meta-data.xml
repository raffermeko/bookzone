<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
  <name>id-allocator-example</name>
  <package-version>0.0.0.0</package-version>
  <description>Example PKG Resource allocator for ids</description>
  <ncs-min-version>3.0</ncs-min-version>

  <!-- It's possible to add more components to the -->
  <!-- same package, multiple services, data providers etc -->

  <required-package>
    <name>resource-manager-example</name>
  </required-package>

  <component>
    <name>Reactive fastmap id  allocation</name>
    <application>
      <java-class-name>com.cisco.nso.idallocator.IdAllocator</java-class-name>
    </application>
  </component>

</ncs-package>
